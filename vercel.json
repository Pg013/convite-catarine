    {
      "version": 2,
      "builds": [
        {
          "src": "src/App.js",
          "use": "@vercel/static-build",
          "config": { "outputDirectory": "build" }
        },
        {
          "src": "backend/server.js",
          "use": "@vercel/node"
        }
      ],
      "routes": [
        {
          "src": "/api/(.*)",
          "dest": "backend/server.js"
        },
        {
          "src": "/(.*)",
          "dest": "src/App.js"
        }
      ]
    }
    
